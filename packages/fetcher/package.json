{
  "name": "fetch",
  "version": "1.0.0",
  "description": "Collect validators information from a beacon node, process and save it to the db.",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "dev": "dotenv -e ../../.env npx tsx src/index.ts",
    "dev:test": "dotenv -e ../../.env.local npx tsx src/test.ts",
    "start": "NODE_OPTIONS='--import tsx' tsx src/index.ts",
    "show-logs": "tail -n 50 -f logs/$(date +\"%d-%m-%Y\").log",
    "has-log-errors": "grep -Ei 'error' \"./logs/$(date +'%d-%m-%Y').log\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "@t3-oss/env-core": "^0.10.1",
    "axios": "^1.7.2",
    "axios-logger": "^2.8.1",
    "axios-rate-limit": "^1.4.0",
    "bitlist": "^1.0.0",
    "bitset": "^5.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.17",
    "ms": "^2.1.3",
    "p-retry": "^6.2.0",
    "pg": "^8.5.1",
    "pino": "^9.3.1",
    "pino-pretty": "^11.2.1",
    "pino-tee": "^0.3.0",
    "rate-limiter-flexible": "^5.0.3",
    "toad-scheduler": "^3.0.1",
    "viem": "^2.23.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.14.200",
    "@types/memoizee": "^0.4.11",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "jest": "^29.7.0",
    "prisma": "5.15.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
